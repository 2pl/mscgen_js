# test 52 - simple "alt within a loop"
msc { a,b,c; 
  a => b; 
  a loop c { 
    b alt c { 
        b -> c [label="-> within alt"]; 
        c >> b [label=">>> within alt"]; 
    } [label="label for alt"]; 
    b >> a [label="within loop"];
  } [label="label for loop"];
  a =>> a [label="happy-the-peppy - outside"];
  ...;
}