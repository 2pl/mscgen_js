# test09 - arcskips
msc {
  a [linecolor="white", label=" "],
  b [linecolor="white", label=" ", arclinecolor="grey"],
  c [linecolor="white", label=" "];
  
  |||;|||;|||;
  b =>> * [arcskip="4"], b =>> * [arcskip="-4"], 
  b =>> * [arcskip="3"], b =>> * [arcskip="-3"], 
  b =>> * [arcskip="2"], b =>> * [arcskip="-2"], 
  b =>> * [arcskip="1"], b =>> * [arcskip="-1"], 
  b =>> *;
  ||| [label="over there", textcolor="#eee"];
  ||| [label="over there", textcolor="#bbb"];
  ||| [label="over there", textcolor="#999"];
  ||| [label="over there", textcolor="#777"];
  --- [linecolor="#777"];
}


