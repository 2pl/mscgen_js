    "mscgen",
    "sequence chart",
    "sequence diagram",
    "xu",
    "msgenny"
  ],
  "engines": {
    "iojs": "^1.6.4"
  },
  "bugs": {
    "url": "https://github.com/sverweij/mscgen_js/issues"
  },
  "homepage": "https://sverweij.github.io/mscgen_js"
