# how mscgen_js works internally
msc {
  
  hscale="0.9";
  
  you, mscgen_js, parser [label="parser (pegjs)"],  renderer, canvg;

  you       => mscgen_js [label="some text"];
  mscgen_js => parser [label="ok?"];
  parser    >> mscgen_js [label="syntax tree"];
  mscgen_js => renderer [label="syntax tree"];
  renderer  => renderer [label="render"];
  renderer  >> mscgen_js;
  mscgen_js >> you [label="vector picture"];
  you       => mscgen_js [label="raster graphics?"];
  mscgen_js => canvg [label="vector picture"];
  canvg     >> mscgen_js [label="raster picture"];
  mscgen_js >> you [label="raster picture"];
}


