{
  "name": "mscgen_js",
  "version": "0.9.76",
  "description": "Turns text into sequence charts. A faithfull implementation of the mscgen sequence chart language in javascript.",
  "dependencies": {
    "amdefine": "^0.1.0",
    "btoa": "^1.1.2",
    "jsdom": "^5.4.2",
    "requirejs": "^2.1.17"
  },
  "devDependencies": {
    "pegjs": "^0.8.0",
    "node-sass": "^3.1.2",
    "mocha": "^2.2.5",
    "codeclimate-test-reporter": "^0.0.4",
    "jshint": "^2.7.0",
    "csslint": "^0.10.0",
    "istanbul": "^0.3.14",
    "plato": "^1.4.0",
    "nsp": "^1.0.1"
  },
  "scripts": {
    "test": "mocha -R dot src/script/test/",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha src/script/test",
    "nsp": "nsp package",
    "plato": "plato -r -d platoreports -x \"parser|test|cli|attic\" src/script/",
    "lint": "jshint src/script --verbose --show-non-errors",
    "codeclimate-cover-submit": "node node_modules/codeclimate-test-reporter/bin/codeclimate.js < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sverweij/mscgen_js"
